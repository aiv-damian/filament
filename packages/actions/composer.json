{
    "name": "aiv/actions",
    "description": "Easily add beautiful action modals to any Livewire component.",
    "license": "MIT",
    "homepage": "https://github.com/filamentphp/filament",
    "support": {
        "issues": "https://github.com/filamentphp/filament/issues",
        "source": "https://github.com/filamentphp/filament"
    },
    "require": {
        "php": "^8.1",
        "aiv/forms": "self.version",
        "aiv/infolists": "self.version",
        "aiv/notifications": "self.version",
        "aiv/support": "self.version",
        "illuminate/contracts": "^10.0",
        "illuminate/database": "^10.0",
        "illuminate/support": "^10.0",
        "spatie/laravel-package-tools": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "Filament\\Actions\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Filament\\Actions\\ActionsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
